<%- include('./adminLayouts/Ahead.ejs') %>

    <h1>MailBox</h1>

    <!-- klasickej for in iteruje přes jendotlivé dokumety v collection, proto [object Object] vypisovalo pole dokumetu a nepoložek v něm -->
    <!--  -->
    <ul>
        <% msg.forEach((m) => { %>
            <li>
                <%= m.name %>
                <%= m.email %>
                <%= m.msg %>
            </li>
        <% }) %>
    </ul>

